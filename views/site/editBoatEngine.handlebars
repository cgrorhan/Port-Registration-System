<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>Tekne Motor Düzenleme</title>

  <!-- Custom fonts for this template-->
  <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
  <!-- Custom styles for this template-->
  <link href="css/sb-admin-2.min.css" rel="stylesheet">
</head>
<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">


    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">
        <div class="container">

          <!-- Outer Row -->
          <div class="row justify-content-center">
      
            <div class="col-xl-10 col-lg-12 col-md-9">
      
              <div class="card o-hidden border-0 shadow-lg my-5">
                <div class="card-body p-0">
                  <!-- Nested Row within Card Body -->
                  <div class="row">
                   
                    <div class="col-lg-12">
                        <div class="p-5">
                          
                          <div>
                            <h1 class="h4 text-gray-900 mb-4">TEKNE MOTORU DÜZENLEME</h1>
                          </div>
                          <form action="/boatEngineRoutes/engineData/{{postEngineData._id}}?_method=PUT" method="POST" id="portRegistrationInfo">
                        
                            <div class="form-group">
                                <input type="hidden" name="_method" value="PUT">
                                <label for="brand"><b>Motorun markasını seçiniz:</b></label>
                               <select id="brand" name="brand" >
                                  <option value="mercury">Mercury</option>
                                  <option value="suzuki">Suzuki</option>
                                  <option value="tomking">Tomking</option>   
                                  <option value="bestway">Bestway</option>
                                  <option value="goldsea">Goldsea</option>
                                  <option value="parsun">Parsun</option>
                                  <option value="tohatsu">Tohatsu</option>
                                  <option value="other">Diğer</option>
                                 
  
  
                                </select>
                            </div>
                            <div class="form-group">
                                <label for="model"><b>Motor modelini giriniz:</b></label>
                                <input type="text" value="{{postEngineData.model}}" name="model" id="model" class="form-control form-control-user" required>

                            </div>
                        
                           <div class="form-group">
                                <label for="hp"><b>Beygir gücünü(hp) giriniz:</b></label>
                                <input type="text" value="{{postEngineData.hp}}" name="hp" id="hp" class="form-control form-control-user" required>

                            </div>
                            <button type="submit" class="btn btn-primary btn-user btn-block">Kaydet</button>
                            <hr>
                          </form>
                        </div>
                      </div>
                  </div>
                </div>
              </div>
      
            </div>
      
          </div>
      
        </div>
      </div>
      <!-- End of Main Content -->

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>



  <!-- Bootstrap core JavaScript-->
  <script src="vendor/jquery/jquery.min.js"></script>
  <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Core plugin JavaScript-->
  <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for all pages-->
  <script src="js/sb-admin-2.min.js"></script>

  <!-- Page level plugins -->
  <script src="vendor/chart.js/Chart.min.js"></script>

  <!-- Page level custom scripts -->
  <script src="js/demo/chart-area-demo.js"></script>
  <script src="js/demo/chart-pie-demo.js"></script>

</body>
</html>